{
	"name": "@kyrregjerstad/vipps-oauth-plugin",
	"version": "1.0.0",
	"description": "A Better Auth plugin for Vipps OAuth",
	"main": "dist/index.js",
	"type": "module",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vitest",
		"build": "tsc",
		"ci": "pnpm run build && pnpm exec biome check && pnpm run test",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check-format": "pnpm exec biome check",
		"check": "tsc --noEmit",
		"test": "vitest run"
	},
	"keywords": [
		"better-auth",
		"vipps",
		"oauth",
		"plugin"
	],
	"author": "Kyrre Gjerstad <hi@kyrre.dev> (https://kyrre.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kyrregjerstad/better-auth-vipps-oauth-plugin.git"
	},
	"bugs": {
		"url": "https://github.com/kyrregjerstad/better-auth-vipps-oauth-plugin/issues"
	},
	"homepage": "https://github.com/kyrregjerstad/better-auth-vipps-oauth-plugin#readme",
	"packageManager": "pnpm@10.15.0",
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@changesets/cli": "^2.29.6",
		"msw": "^2.10.5",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"better-auth": "^1.1.0"
	},
	"dependencies": {
		"zod": "^4.1.1"
	}
}
